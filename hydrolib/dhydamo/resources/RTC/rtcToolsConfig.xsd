        <sequence>
            <element name="HUp" type="rtc:TimeSeriesSimpleType"/>
            <element name="DeltaHUp" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
            <element name="HDown" type="rtc:TimeSeriesSimpleType"/>
            <element name="DeltaHDown" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
            <choice>
                <element name="DConst" type="double"/>
                <element name="DSeries" type="rtc:TimeSeriesSimpleType"/>
                <element name="D" type="rtc:TimeSeriesSimpleType">
                    <annotation>
                        <documentation>deprecated</documentation>
                    </annotation>
                </element>
            </choice>
        </sequence>
    </complexType>
    <complexType name="OrificeOutputComplexType">
        <sequence>
            <element name="Q" type="rtc:TimeSeriesSimpleType"/>
            <element name="D" type="rtc:TimeSeriesSimpleType"/>
            <element name="flowType" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
        </sequence>
    </complexType>
    <complexType name="BranchGradientComplexType">
        <sequence>
            <element name="storageUp" type="rtc:TimeSeriesSimpleType"/>
            <element name="surfaceAreaUp" type="rtc:TimeSeriesSimpleType"/>
            <element name="storageDown" type="rtc:TimeSeriesSimpleType"/>
            <element name="surfaceAreaDown" type="rtc:TimeSeriesSimpleType"/>
        </sequence>
    </complexType>
    <complexType name="RiverWeirComplexType">
        <sequence>
            <element name="width" type="double"/>
            <element name="exponentWeirFormula" type="double" default="1.5" minOccurs="0"/>
            <element name="submergedFlowRatio" type="double" default="1.5" minOccurs="0"/>
            <element name="submergedFlowFactor" type="double" default="1.0" minOccurs="0"/>
            <element name="flowDirection" type="rtc:flowDirectionEnumStringType" minOccurs="0"/>
            <element name="input" type="rtc:RiverWeirInputComplexType"/>
            <element name="output" type="rtc:RiverWeirOutputComplexType"/>
        </sequence>
    </complexType>
    <complexType name="RiverWeirInputComplexType">
        <sequence>
            <element name="HUp" type="rtc:TimeSeriesSimpleType"/>
            <element name="DeltaHUp" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
            <element name="HDown" type="rtc:TimeSeriesSimpleType"/>
            <element name="DeltaHDown" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
            <choice>
                <element name="CConst" type="double"/>
                <element name="CSeries" type="rtc:TimeSeriesSimpleType"/>
            </choice>
        </sequence>
    </complexType>
    <complexType name="RiverWeirOutputComplexType">
        <sequence>
            <element name="Q" type="rtc:TimeSeriesSimpleType"/>
            <element name="C" type="rtc:TimeSeriesSimpleType"/>
            <element name="flowType" type="rtc:TimeSeriesSimpleType" minOccurs="0"/>
        </sequence>
    </complexType>
    <simpleType name="poolRoutingEnumStringType">
        <annotation>
            <documentation>Type of pool routing.</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="ForwardEuler"/>
            <enumeration value="BackwardEuler"/>
            <enumeration value="Theta"/>
        </restriction>
    </simpleType>
    <simpleType name="interpolationOptionEnumStringType">
        <annotation>
            <documentation>Type of interpolation</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="BLOCK"/>
            <enumeration value="LINEAR"/>
        </restriction>
    </simpleType>
    <simpleType name="limiterOptionEnumStringType">
        <annotation>
            <documentation>Type of limiting</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="PERCENTAGE"/>
            <enumeration value="ABSOLUTE"/>
        </restriction>
    </simpleType>
    <simpleType name="timeRelativeEnumStringType">
        <annotation>
            <documentation>Value option</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="ABSOLUTE"/>
            <enumeration value="RELATIVE"/>
        </restriction>
    </simpleType>
    <simpleType name="caseLangetenEnumStringType">
        <restriction base="int">
            <enumeration value="1"/>
            <enumeration value="2"/>
        </restriction>
    </simpleType>
    <simpleType name="flowDirectionEnumStringType">
        <annotation>
            <documentation>flow direction</documentation>
        </annotation>
        <union>
            <simpleType>
                <restriction base="string">
                    <enumeration value="POSITIVE"/>
                    <enumeration value="NEGATIVE"/>
                    <enumeration value="BOTH"/>
                </restriction>
            </simpleType>
            <simpleType>
                <restriction base="string">
                    <pattern value="[$][a-zA-Z0-9.()*/+-_]+[$]"/>
                </restriction>
            </simpleType>
        </union>
    </simpleType>
    <simpleType name="logicalOperatorEnumStringType">
        <annotation>
            <documentation>logical operator</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="AND"/>
            <enumeration value="OR"/>
            <enumeration value="XOR"/>
        </restriction>
    </simpleType>
    <simpleType name="relationalOperatorEnumStringType">
        <annotation>
            <documentation>relational operator</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="Less"/>
            <enumeration value="LessEqual"/>
            <enumeration value="Equal"/>
            <enumeration value="Unequal"/>
            <enumeration value="GreaterEqual"/>
            <enumeration value="Greater"/>
        </restriction>
    </simpleType>
    <simpleType name="MathematicalOperatorEnumStringType">
        <annotation>
            <documentation>mathematical operators</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="+"/>
            <enumeration value="-"/>
            <enumeration value="*"/>
            <enumeration value="/"/>
            <enumeration value="min"/>
            <enumeration value="max"/>
            <enumeration value="^"/>
        </restriction>
    </simpleType>
    <simpleType name="SpatialEnumStringType">
        <annotation>
            <documentation>options for spatial schetization</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="CENTRAL"/>
            <enumeration value="UPWIND"/>
        </restriction>
    </simpleType>
    <simpleType name="CapacityReferenceEnumStringType">
        <annotation>
            <documentation>reference for capacity</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="UPSTREAMLEVEL"/>
            <enumeration value="HEAD"/>
        </restriction>
    </simpleType>
    <simpleType name="inputReferenceEnumStringType">
        <restriction base="string">
            <enumeration value="EXPLICIT"/>
            <enumeration value="IMPLICIT"/>
        </restriction>
    </simpleType>
    <simpleType name="BranchTypeEnumStringType">
        <restriction base="string">
            <enumeration value="KINEMATIC"/>
            <enumeration value="DIFFUSIVE"/>
            <enumeration value="DIFFUSIVEPLUSLOCAL"/>
        </restriction>
    </simpleType>
    <simpleType name="EquationEnumStringType">
        <restriction base="string">
            <enumeration value="KINEMATIC"/>
            <enumeration value="DIFFUSIVE"/>
            <enumeration value="INERTIAL"/>
        </restriction>
    </simpleType>
    <simpleType name="MergerSplitterEnumStringType">
        <restriction base="string">
            <enumeration value="AVERAGE"/>
            <enumeration value="DATAHIERARCHY"/>
            <enumeration value="SUM"/>
        </restriction>
    </simpleType>
    <simpleType name="HydraulicModelModeEnumStringType">
        <restriction base="string">
            <enumeration value="SEQUENTIAL_EXPLICIT"/>
            <enumeration value="SEQUENTIAL_IMPLICIT"/>
            <enumeration value="SIMULTANEOUS"/>
        </restriction>
    </simpleType>
    <simpleType name="transferFunctionEnumStringType">
        <annotation>
            <documentation>Type of transfer function.</documentation>
        </annotation>
        <restriction base="string">
            <enumeration value="Linear"/>
            <enumeration value="SigmoidLogistic"/>
            <enumeration value="Tansig"/>
        </restriction>
    </simpleType>
    <complexType name="PidComplexType">
        <sequence>
            <element name="mode" default="NATIVE" minOccurs="0">
                <annotation>
                    <documentation>pid mode, either "NATIVE" or "SOBEK2" or "PIDPOS" or PIDVEL"</documentation>
                </annotation>
                <simpleType>
                    <restriction base="string">
                        <enumeration value="NATIVE"/>
                        <enumeration value="SOBEK2"/>
                        <enumeration value="PIDPOS"/>
                        <enumeration value="PIDVEL"/>
                    </restriction>
                </simpleType>
            </element>
            <element name="settingMin" type="double">
                <annotation>
                    <documentation>minimum setting of the actuator</documentation>
                </annotation>
            </element>
            <element name="settingMax" type="double">
                <annotation>
                    <documentation>maximum setting of the actuator</documentation>
                </annotation>
            </element>
            <element name="settingMaxSpeed" type="double">
                <annotation>
                    <documentation>maximum speed of the actuation in [unit/s]</documentation>
                </annotation>
            </element>
            <element name="kp" type="double">
                <annotation>
                    <documentation>factor on the proportional part kp*e</documentation>
                </annotation>
            </element>
            <element name="ki" type="double">
                <annotation>
                    <documentation>factor on the integral part ki*integral(e)dt</documentation>
                </annotation>
            </element>
            <element name="kd" type="double">
                <annotation>
                    <documentation>fatcor on differential part kd*de/dt</documentation>
                </annotation>
            </element>
            <element name="input" type="rtc:InputPidComplexType"/>
            <element name="output" type="rtc:OutputPidComplexType"/>
        </sequence>
        <attribute name="id" type="string" use="required"/>
        <attribute name="name" type="string" use="optional"/>
    </complexType>
    <complexType name="InputPidComplexType">
        <sequence>
            <element name="x" type="rtc:TimeSeriesSimpleType">
                <annotation>
                    <documentation>controllable variable</documentation>
                </annotation>
            </element>
            <choice>
                <element name="setpointSeries" type="rtc:TimeSeriesSimpleType"/>
                <element name="setpointValue" type="double"/>
            </choice>
            <element name="disturbance" minOccurs="0">
                <complexType>
                    <simpleContent>
                        <extension base="rtc:TimeSeriesSimpleType">
                            <attribute name="factor" type="double" use="optional" default="1.0"/>
                        </extension>
                    </simpleContent>
                </complexType>
            </element>
        </sequence>
    </complexType>
    <complexType name="OutputPidComplexType">
        <sequence>
            <element name="y" type="rtc:TimeSeriesSimpleType">
                <annotation>
                    <documentation>setting of the actuator</documentation>
                </annotation>
            </element>
            <element name="integralPart" type="rtc:TimeSeriesSimpleType">
                <annotation>
                    <documentation>memory of integral part: integral(e)dt</documentation>
                </annotation>
            </element>
            <element name="differentialPart" type="rtc:TimeSeriesSimpleType">
                <annotation>
                    <documentation>memory of differential part (in fact e of the last time step)</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="TriggerControllerComplexType">
        <sequence>
            <element name="input" type="rtc:TimeSeriesSimpleType"/>
            <element name="value" type="integer"/>
        </sequence>
    </complexType>
    <simpleType name="LayerIDSimpleType">
        <restriction base="string">
            <pattern value="[L][0-9]+"/>
        </restriction>
    </simpleType>
    <simpleType name="NeuronIDSimpleType">
        <restriction base="string">
            <pattern value="[L][0-9]+[N][0-9]+"/>
        </restriction>
    </simpleType>
    <simpleType name="UnitTypeEnumStringType">
        <restriction base="string">
            <enumeration value="SI"/>
            <enumeration value="Imperial"/>
        </restriction>
    </simpleType>
</schema>
