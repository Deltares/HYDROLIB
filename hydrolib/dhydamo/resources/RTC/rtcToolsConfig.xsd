                <element name="turbineEfficiencyTable2DExternal" type="string"/>
            </choice>
            <element name="input" type="rtc:ReservoirCompactInputComplexType">
                <annotation>
                    <documentation>input time series</documentation>
                </annotation>
            </element>
            <element name="output" type="rtc:ReservoirCompactOutputComplexType">
                <annotation>
                    <documentation>output time series</documentation>
                </annotation>
            </element>
